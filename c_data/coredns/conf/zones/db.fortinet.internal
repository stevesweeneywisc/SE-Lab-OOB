$ORIGIN fortinet.internal.
$TTL 1h
@             IN  SOA (
                  ns     ; primary nameserver
                  .      ; zone-admin email
                  1      ; serial number
                  24h    ; refresh interval
                  2h     ; retry interval
                  1000h  ; expire interval
                  10m    ; negative TTL
              )

ns         5m IN  A   10.100.55.80
oob           IN  CNAME ns
ubuntu-oob    IN  CNAME oob

pve01         IN  A   172.16.3.121

tcgui         IN  A   172.16.3.90
ubuntu-tcgui  IN  CNAME tcgui

ubuntu-00     IN  A   10.100.55.70
ubuntu-01     IN  A   10.100.55.71
ubuntu-02     IN  A   10.100.55.72
ubuntu-03     IN  A   10.100.55.73

ubuntu-11     IN  A   10.100.55.81

ubuntu-21     IN  A   10.100.55.98

fgt-hub-01a   IN  A   10.100.55.51
fgt-hub-01b   IN  A   10.100.55.52
fgt-hub-02a   IN  A   10.100.55.53
fgt-hub-02b   IN  A   10.100.55.54
fgt-branch-01 IN  A   10.100.55.55
fgt-branch-02 IN  A   10.100.55.56
fgt-branch-03 IN  A   10.100.55.57

fmg           IN  A   10.100.55.12 
faz           IN  A   10.100.55.2
